{
  "name": "oss-explorer",
  "version": "1.0.7",
  "private": false,
  "main": "dist/oss-explorer.umd.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "library": "vue-cli-service build --target lib ./src/explorer.js",
    "deploy": "npm run build && gh-pages -d dist --remote origin",
    "preversion": "npm run library",
    "postversion": "npm publish && git push && git push --tags",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-mocha": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "dayjs": "^1.8.16",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-icon-vectors": "^1.0.0",
    "gh-pages": "^2.1.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^8.1.5",
    "qs": "^6.8.0",
    "vanilla-lazyload": "^12.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
